            android:screenOrientation="portrait"
            android:theme="@style/LoanWithdrawal" />
        <activity
            android:name="za.co.woolworths.financial.services.android.ui.activities.MessagesActivity"
            android:parentActivityName="za.co.woolworths.financial.services.android.ui.activities.WOneAppBaseActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Messages">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="za.co.woolworths.financial.services.android.ui.activities.WOneAppBaseActivity" />
        </activity>
        <activity
            android:name="za.co.woolworths.financial.services.android.ui.activities.WOneAppBaseActivity"
