            android:theme="@style/CLI"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="za.co.woolworths.financial.services.android.ui.activities.ShoppingListActivity"
            android:screenOrientation="portrait"
            android:theme="@style/ShoppingList" />
        <activity
            android:name="za.co.woolworths.financial.services.android.ui.activities.CLIActivity"
            android:screenOrientation="portrait"
            android:theme="@style/ShoppingList" />
        <activity
            android:name="za.co.woolworths.financial.services.android.ui.activities.LoanWithdrawalActivity"
            android:screenOrientation="portrait"
