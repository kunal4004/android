            <enum name="vertical" value="3"/>
        </attr>
        <attr name="leftOverlay" format="reference"/>
        <attr name="rightOverlay" format="reference"/>
        <attr name="bottomOverlay" format="reference"/>
        <attr name="topOverlay" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CurrencyEditText">
        <attr name="currencySymbol" format="string"/>
        <attr name="localeTag" format="string"/>
        <attr name="useCurrencySymbolAsHint" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="NotificationBadge">
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
        <attr name="nbBackground" format="reference" />
        <attr name="nbAnimationEnabled" format="boolean" />
        <attr name="nbAnimationDuration" format="integer" />
        <attr name="nbMaxTextLength" format="integer" />
        <attr name="nbEllipsizeText" format="string" />
    </declare-styleable>

    <declare-styleable name="DragToClose">
        <attr name="dragToClose_draggableView" format="reference" />
        <attr name="dragToClose_draggableContainer" format="reference" />
        <attr name="dragToClose_finishActivity" format="boolean" />
        <attr name="dragToClose_closeOnClick" format="boolean" />
    </declare-styleable>

