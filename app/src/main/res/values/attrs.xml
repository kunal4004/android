        </attr>

        <attr name="wv_curvedArcDirectionFactor" format="float"/>

        <attr name="wv_refractRatio" format="float"/>

        <attr name="wv_curvedRefractRatio" format="float"/>


        <attr name="wv_drawSelectedRect" format="boolean"/>
