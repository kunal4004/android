<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:paddingBottom="@dimen/twenty_four_dp">

    <TextView
        android:id="@+id/myAddressTitle"
        style="@style/futura_semi_bold_black"
        android:contentDescription="@string/cd_ccd_labledeliverydetailstitle"
        android:letterSpacing="0.1"
        android:paddingTop="@dimen/two_dp"
        android:paddingBottom="@dimen/two_dp"
        android:text="@string/my_address_details"
        android:textSize="@dimen/twenty_sp"
        app:layout_constraintRight_toRightOf="@id/right_guideline"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toTopOf="@id/top_guideline" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView"
        style="@style/credi_card_delivery_edittext_style"
        android:layout_marginTop="@dimen/fifteen_dp"
        android:drawableStart="@drawable/mapview"
        android:drawablePadding="@dimen/eight_dp"
        android:hint="@string/places_autocomplete_tv_hint"
        android:maxLines="1"
        app:layout_constraintEnd_toEndOf="@id/right_guideline"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/myAddressTitle" />

    <TextView
        android:id="@+id/autocompletePlaceErrorMsg"
        style="@style/myriad_pro_regular"
        android:layout_marginTop="@dimen/fifteen_dp"
        android:lineSpacingMultiplier="1.40"
        android:text="@string/places_address_error_msg"
        android:textColor="@color/input_error_color_code"
        android:textSize="@dimen/fourteen_sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@id/right_guideline"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/autoCompleteTextView" />

    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/addressNicknameShimmerFrameLayout"
        style="@style/myriad_pro_regular_black"
        android:layout_width="wrap_content"
        android:layout_marginTop="@dimen/twenty_four_dp"
        android:background="@color/shimmer_view_background"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/autocompletePlaceErrorMsg">

        <TextView
            android:id="@+id/addressNicknamePlaceHolder"
            style="@style/myriad_pro_regular_black"
            android:layout_width="match_parent"
            android:background="@color/white"
            android:contentDescription="@string/cd_ccd_labelsuburb"
            android:paddingTop="@dimen/two_dp"
            android:paddingBottom="@dimen/two_dp"
            android:text="@string/address_nickname_text_view"
            android:textSize="@dimen/fourteen_sp" />

    </com.facebook.shimmer.ShimmerFrameLayout>

    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/addressNicknameEditTextShimmerFrameLayout"
        style="@style/shimmer_view_edittext_style"
        android:background="@color/shimmer_view_background"
        app:layout_constraintEnd_toEndOf="@id/right_guideline"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/addressNicknameShimmerFrameLayout">

        <EditText
            android:id="@+id/addressNicknameEditText"
            style="@style/checkout_edittext_style"
            android:layout_width="match_parent"
            android:maxLength="@integer/thirty" />
    </com.facebook.shimmer.ShimmerFrameLayout>

    <TextView
        android:id="@+id/addressNicknameErrorMsg"
        style="@style/myriad_pro_regular"
        android:layout_marginTop="@dimen/fifteen_dp"
        android:lineSpacingMultiplier="1.40"
        android:text="@string/address_nickname_error_msg"
        android:textColor="@color/input_error_color_code"
        android:textSize="@dimen/fourteen_sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@id/right_guideline"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/addressNicknameEditTextShimmerFrameLayout" />

    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/unitComplexPlaceHolderShimmerFrameLayout"
        style="@style/myriad_pro_regular_black"
        android:layout_width="wrap_content"
        android:layout_marginTop="@dimen/twenty_four_dp"
        android:background="@color/shimmer_view_background"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/addressNicknameErrorMsg">

        <TextView
            android:id="@+id/unitComplexFloorPlaceHolder"
            style="@style/myriad_pro_regular_black"
            android:layout_width="match_parent"
            android:background="@color/white"
            android:contentDescription="@string/cd_ccd_labelstreetaddress"
            android:paddingTop="@dimen/two_dp"
            android:paddingBottom="@dimen/two_dp"
            android:text="@string/unit_complex_floor_text_view"
            android:textSize="@dimen/fourteen_sp" />
    </com.facebook.shimmer.ShimmerFrameLayout>

    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/unitComplexEditTextShimmerFrameLayout"
        style="@style/shimmer_view_edittext_style"
        android:background="@color/shimmer_view_background"
        app:layout_constraintEnd_toEndOf="@id/right_guideline"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/unitComplexPlaceHolderShimmerFrameLayout">

        <EditText
            android:id="@+id/unitComplexFloorEditText"
            style="@style/checkout_edittext_style"
            android:layout_width="match_parent"
            android:contentDescription="@string/cd_ccd_txtstreetaddress"
            android:hint="@string/optional_hint"
            android:maxLength="@integer/fifty" />
    </com.facebook.shimmer.ShimmerFrameLayout>

    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/suburbPlaceHolderShimmerFrameLayout"
        style="@style/myriad_pro_regular_black"
        android:layout_width="wrap_content"
        android:layout_marginTop="@dimen/twenty_four_dp"
        android:background="@color/shimmer_view_background"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/unitComplexEditTextShimmerFrameLayout">

        <TextView
            android:id="@+id/suburbPlaceHolder"
            style="@style/myriad_pro_regular_black"
            android:layout_width="match_parent"
            android:background="@color/white"
            android:contentDescription="@string/cd_ccd_labelsuburb"
            android:paddingTop="@dimen/two_dp"
            android:paddingBottom="@dimen/two_dp"
            android:text="@string/suburb_colon_text_view"
            android:textSize="@dimen/fourteen_sp" />
    </com.facebook.shimmer.ShimmerFrameLayout>

    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/selectSuburbLayoutShimmerFrameLayout"
        android:layout_width="0dp"
        android:layout_height="@dimen/fourty_eight_dp"
        android:layout_marginTop="@dimen/six_dp"
        android:background="@color/shimmer_view_background"
        app:layout_constraintLeft_toLeftOf="@+id/left_guideline"
        app:layout_constraintRight_toRightOf="@+id/right_guideline"
        app:layout_constraintTop_toBottomOf="@id/suburbPlaceHolderShimmerFrameLayout">

        <RelativeLayout
            android:id="@+id/selectSuburbLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/fourty_eight_dp"
            android:background="@drawable/input_non_editable_edit_text"
            android:clickable="false"
            android:focusable="true"
            android:paddingEnd="@dimen/nine_dp">

            <EditText
                android:id="@+id/suburbEditText"
                style="@style/myriad_pro_regular"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@+id/progressbarGetSuburb"
                android:background="@drawable/input_non_editable_half_edit_text"
                android:focusable="false"
                android:paddingStart="@dimen/sixteen_dp"
                android:paddingEnd="@dimen/thirty_two_dp"
                android:textColor="@color/offer_title"
                android:textSize="@dimen/sixteen_sp" />

            <ProgressBar
                android:id="@+id/progressbarGetSuburb"
                style="?android:attr/progressBarStyleLarge"
                android:layout_width="@dimen/twenty_four_dp"
                android:layout_height="@dimen/twenty_four_dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:focusable="false"
                android:indeterminateTint="@color/black"
                android:visibility="invisible" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/dropdownGetSuburbImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:background="@drawable/icon_dropdown"
                android:focusable="false"
                android:visibility="invisible" />
        </RelativeLayout>
    </com.facebook.shimmer.ShimmerFrameLayout>

    <TextView
        android:id="@+id/suburbNameErrorMsg"
        style="@style/myriad_pro_regular"
        android:layout_marginTop="@dimen/fifteen_dp"
        android:lineSpacingMultiplier="1.40"
        android:text="@string/suburb_name_error_msg"
        android:textColor="@color/input_error_color_code"
        android:textSize="@dimen/fourteen_sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@id/right_guideline"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/selectSuburbLayoutShimmerFrameLayout" />

    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/provincePlaceHolderShimmerFrameLayout"
        style="@style/myriad_pro_regular_black"
        android:layout_width="wrap_content"
        android:layout_marginTop="@dimen/twenty_four_dp"
        android:background="@color/shimmer_view_background"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/suburbNameErrorMsg">

        <TextView
            android:id="@+id/provincePlaceHolder"
            style="@style/myriad_pro_regular_black"
            android:layout_width="match_parent"
            android:background="@color/white"
            android:contentDescription="@string/cd_ccd_labelprovince"
            android:paddingTop="@dimen/two_dp"
            android:paddingBottom="@dimen/two_dp"
            android:text="@string/province_colon_text_view"
            android:textSize="@dimen/fourteen_sp" />
    </com.facebook.shimmer.ShimmerFrameLayout>

    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/selectProvinceLayoutShimmerFrameLayout"
        android:layout_width="0dp"
        android:layout_height="@dimen/fourty_eight_dp"
        android:layout_marginTop="@dimen/six_dp"
        android:background="@color/shimmer_view_background"
        app:layout_constraintLeft_toLeftOf="@+id/left_guideline"
        app:layout_constraintRight_toRightOf="@+id/right_guideline"
        app:layout_constraintTop_toBottomOf="@id/provincePlaceHolderShimmerFrameLayout">

        <RelativeLayout
            android:id="@+id/selectProvinceLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/fourty_eight_dp"
            android:background="@drawable/input_non_editable_edit_text"
            android:clickable="false"
            android:focusable="true"
            android:paddingEnd="@dimen/nine_dp">

            <EditText
                android:id="@+id/provinceAutocompleteEditText"
                style="@style/myriad_pro_regular"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@+id/progressbarGetProvinces"
                android:background="@drawable/input_non_editable_half_edit_text"
                android:clickable="false"
                android:focusable="false"
                android:paddingStart="@dimen/sixteen_dp"
                android:paddingEnd="@dimen/thirty_two_dp"
                android:textColor="@color/offer_title"
                android:textSize="@dimen/sixteen_sp" />

            <ProgressBar
                android:id="@+id/progressbarGetProvinces"
                style="?android:attr/progressBarStyleLarge"
                android:layout_width="@dimen/twenty_four_dp"
                android:layout_height="@dimen/twenty_four_dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:focusable="false"
                android:indeterminateTint="@color/black"
                android:visibility="invisible" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/dropdownGetProvincesImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:background="@drawable/icon_dropdown"
                android:focusable="false"
                android:visibility="invisible" />
        </RelativeLayout>
    </com.facebook.shimmer.ShimmerFrameLayout>

    <TextView
        android:id="@+id/provinceNameErrorMsg"
        style="@style/myriad_pro_regular"
        android:layout_marginTop="@dimen/fifteen_dp"
        android:lineSpacingMultiplier="1.40"
        android:text="@string/province_name_error_msg"
        android:textColor="@color/input_error_color_code"
        android:textSize="@dimen/fourteen_sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@id/right_guideline"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/selectProvinceLayoutShimmerFrameLayout" />

    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/postalCodePlaceHolderShimmerFrameLayout"
        style="@style/myriad_pro_regular_black"
        android:layout_width="wrap_content"
        android:layout_marginTop="@dimen/twenty_four_dp"
        android:background="@color/shimmer_view_background"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/provinceNameErrorMsg">

        <TextView
            android:id="@+id/postalCodePlaceHolder"
            style="@style/myriad_pro_regular_black"
            android:layout_width="match_parent"
            android:background="@color/white"
            android:contentDescription="@string/cd_ccd_labelpostalcode"
            android:paddingTop="@dimen/two_dp"
            android:paddingBottom="@dimen/two_dp"
            android:text="@string/postal_code_colon_text_view"
            android:textSize="@dimen/fourteen_sp" />
    </com.facebook.shimmer.ShimmerFrameLayout>

    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/postalCodeShimmerFrameLayout"
        style="@style/shimmer_view_edittext_style"
        android:background="@color/shimmer_view_background"
        app:layout_constraintEnd_toEndOf="@id/right_guideline"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/postalCodePlaceHolderShimmerFrameLayout">

        <EditText
            android:id="@+id/postalCode"
            style="@style/checkout_edittext_style"
            android:layout_width="match_parent"
            android:background="@color/checkout_delivering_title_background"
            android:contentDescription="@string/cd_ccd_txtpostalcode"
            android:editable="false"
            android:imeOptions="actionDone"
            android:textColor="@color/non_editable_edit_text_text_color" />
    </com.facebook.shimmer.ShimmerFrameLayout>

    <TextView
        android:id="@+id/postalCodeTextErrorMsg"
        style="@style/myriad_pro_regular"
        android:layout_marginTop="@dimen/fifteen_dp"
        android:lineSpacingMultiplier="1.40"
        android:text="@string/postal_code_error_msg"
        android:textColor="@color/input_error_color_code"
        android:textSize="@dimen/fourteen_sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@id/right_guideline"
        app:layout_constraintStart_toStartOf="@id/left_guideline"
        app:layout_constraintTop_toBottomOf="@id/postalCodeShimmerFrameLayout" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/left_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="@dimen/thirty_two_dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/right_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="@dimen/thirty_two_dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="@dimen/twenty_four_dp" />
</androidx.constraintlayout.widget.ConstraintLayout>
