<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              android:layout_marginTop="?attr/actionBarSize"
              android:background="@color/grey_bg"
              tools:context="za.co.woolworths.financial.services.android.ui.fragments.contact_us.main_list.ContactUsFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/generalEnq"
        android:minHeight="60dp"
        android:padding="16dp"
        android:background="@drawable/contact_us_item_bg"
        android:layout_marginTop="16dp"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_toLeftOf="@+id/img1"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            >
            <za.co.woolworths.financial.services.android.ui.views.WTextView
                style="@style/contact_us_item_header_textview"
                android:text="@string/txt_general_enquiry"
                />
            <za.co.woolworths.financial.services.android.ui.views.WTextView
                style="@style/contact_us_item_desc_textview"
                android:text="@string/general_enquiry_desc"
                android:layout_marginTop="5dp"
                />
        </LinearLayout>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/img1"
            android:background="@drawable/ic_caret_black"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            />

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/woolworthsOnline"
        android:minHeight="60dp"
        android:padding="16dp"
        android:background="@drawable/contact_us_item_bg"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_toLeftOf="@+id/img2"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            >
            <za.co.woolworths.financial.services.android.ui.views.WTextView
                style="@style/contact_us_item_header_textview"
                android:text="@string/woolworths_online"
                />
            <za.co.woolworths.financial.services.android.ui.views.WTextView
                style="@style/contact_us_item_desc_textview"
                android:text="@string/website_and_online_shoppping"
                android:layout_marginTop="5dp"
                />
        </LinearLayout>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/img2"
            android:background="@drawable/ic_caret_black"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            />

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/wRewards"
        android:minHeight="70dp"
        android:padding="16dp"
        android:background="@drawable/contact_us_item_bg"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_toLeftOf="@+id/img3"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            >
            <za.co.woolworths.financial.services.android.ui.views.WTextView
                style="@style/contact_us_item_header_textview"
                android:text="@string/contact_us_wrewards"
                />

        </LinearLayout>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/img3"
            android:background="@drawable/ic_caret_black"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            />

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mySchoolEnq"
        android:minHeight="70dp"
        android:padding="16dp"
        android:background="@drawable/contact_us_item_bg"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_toLeftOf="@+id/img4"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            >
            <za.co.woolworths.financial.services.android.ui.views.WTextView
                style="@style/contact_us_item_header_textview"
                android:text="@string/contact_us_myschool"
                />
        </LinearLayout>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/img4"
            android:background="@drawable/ic_caret_black"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#1a000000"
        />

</LinearLayout>
