<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#efefef"
    android:paddingTop="?attr/actionBarSize"
    android:scrollbars="none"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="16dp"
        >
    <za.co.woolworths.financial.services.android.ui.views.WTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="68dp"
        android:paddingTop="20dp"
        android:paddingLeft="16dp"
        android:text="@string/call_us"
        android:textColor="@color/black"
        app:WTextView_font="medium_bold"
        android:textSize="12sp"
        android:letterSpacing="0.2"
        android:gravity="center_vertical"
        />
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#0a000000"
            />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/localCaller"
        android:minHeight="50dp"
        android:paddingLeft="16dp"
        android:paddingRight="8dp"
        android:background="@color/white"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_call24"
            android:id="@+id/localCallerImg"
            android:layout_centerVertical="true"
            />
        <za.co.woolworths.financial.services.android.ui.views.WTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/localCallerImg"
            android:layout_marginLeft="16dp"
            android:text="@string/txt_local_caller"
            android:textSize="15sp"
            android:textColor="@color/black"
            app:WTextView_font="myriadpro_regular"
            android:layout_centerVertical="true"
            />
        <za.co.woolworths.financial.services.android.ui.views.WTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:text="@string/online_local_caller_number"
            android:textSize="15sp"
            android:textColor="#4d000000"
            app:WTextView_font="myriadpro_regular"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:drawableRight="@drawable/ic_caret_black"
            android:gravity="center_vertical"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#0a000000"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/internationalCaller"
        android:minHeight="50dp"
        android:paddingLeft="16dp"
        android:paddingRight="8dp"
        android:background="@color/white"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_call24"
            android:id="@+id/internationalCallerImg"
            android:layout_centerVertical="true"
            />
        <za.co.woolworths.financial.services.android.ui.views.WTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/internationalCallerImg"
            android:layout_marginLeft="16dp"
            android:text="@string/txt_international_caller"
            android:textSize="15sp"
            android:textColor="@color/black"
            app:WTextView_font="myriadpro_regular"
            android:layout_centerVertical="true"
            />
        <za.co.woolworths.financial.services.android.ui.views.WTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:text="@string/online_inter_national_caller_number"
            android:textSize="15sp"
            android:textColor="#4d000000"
            app:WTextView_font="myriadpro_regular"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:drawableRight="@drawable/ic_caret_black"
            android:gravity="center_vertical"
            />
    </RelativeLayout>
    <za.co.woolworths.financial.services.android.ui.views.WTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="68dp"
        android:paddingTop="20dp"
        android:background="@drawable/border_top_and_bottom"
        android:paddingStart="16dp"
        android:paddingEnd="16dp"
        android:text="@string/txt_lockdown_call_center_opening_hours"
        android:textColor="@color/black"
        app:WTextView_font="medium_bold"
        android:textSize="12sp"
        android:letterSpacing="0.2"
        android:gravity="center_vertical"
        android:textAllCaps="true"
        />

    <LinearLayout
        style="@style/contact_us_operating_hours_layout"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_time24"
            android:layout_centerVertical="true"
            />
        <za.co.woolworths.financial.services.android.ui.views.WTextView
            style="@style/contact_us_operating_hours_textview"
            android:text="@string/online_call_center_opening_hours"
            />
    </LinearLayout>
    <za.co.woolworths.financial.services.android.ui.views.WTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="68dp"
        android:paddingTop="20dp"
        android:background="@drawable/border_top_and_bottom"
        android:paddingLeft="16dp"
        android:text="@string/email_us"
        android:textColor="@color/black"
        app:WTextView_font="medium_bold"
        android:textSize="12sp"
        android:letterSpacing="0.2"
        android:gravity="center_vertical"
        android:textAllCaps="true"
        />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/onlineShop"
            android:minHeight="70dp"
            android:padding="16dp"
            android:background="@color/white"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/email1"
                android:background="@drawable/message_24"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_toLeftOf="@+id/img1"
                android:layout_toRightOf="@+id/email1"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                >
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_header_textview"
                    android:text="@string/online_shop_reg_login"
                    />
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_desc_textview"
                    android:layout_marginTop="5dp"
                    android:text="@string/email_online_shop"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/img1"
                android:background="@drawable/ic_caret_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#0a000000"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/deliveryQueries"
            android:minHeight="70dp"
            android:padding="16dp"
            android:background="@color/white"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/email2"
                android:background="@drawable/message_24"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_toLeftOf="@+id/img2"
                android:layout_toRightOf="@+id/email2"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                >
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_header_textview"
                    android:text="@string/txt_delivery_queries"
                    />
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_desc_textview"
                    android:layout_marginTop="5dp"
                    android:text="@string/email_online_shop"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/img2"
                android:background="@drawable/ic_caret_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#0a000000"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/technicalProblem"
            android:minHeight="70dp"
            android:padding="16dp"
            android:background="@color/white"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/email3"
                android:background="@drawable/message_24"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_toLeftOf="@+id/img3"
                android:layout_toRightOf="@+id/email3"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                >
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_header_textview"
                    android:text="@string/txt_general_technical_problem"
                    />
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_desc_textview"
                    android:layout_marginTop="5dp"
                    android:text="@string/email_online_shop"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/img3"
                android:background="@drawable/ic_caret_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#0a000000"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/orderQueries"
            android:minHeight="70dp"
            android:padding="16dp"
            android:background="@color/white"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/email4"
                android:background="@drawable/message_24"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_toLeftOf="@+id/img4"
                android:layout_toRightOf="@+id/email4"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                >
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_header_textview"
                    android:text="@string/txt_order_queries"
                    />
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_desc_textview"
                    android:layout_marginTop="5dp"
                    android:text="@string/email_online_shop"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/img4"
                android:background="@drawable/ic_caret_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />

        </RelativeLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#0a000000"
        />



</LinearLayout>
    </ScrollView>