<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="#efefef"
              android:scrollbars="none"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="16dp"
        >
    <za.co.woolworths.financial.services.android.ui.views.WTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="68dp"
        android:paddingTop="20dp"
        android:paddingLeft="16dp"
        android:text="@string/call_us"
        android:textColor="@color/black"
        app:WTextView_font="medium_bold"
        android:textSize="12sp"
        android:letterSpacing="0.2"
        android:gravity="center_vertical"
        />
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#0a000000"
            />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/localCaller"
        android:minHeight="50dp"
        android:paddingLeft="16dp"
        android:paddingRight="8dp"
        android:background="@color/white"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_call24"
            android:id="@+id/localCallerImg"
            android:layout_centerVertical="true"
            />
        <za.co.woolworths.financial.services.android.ui.views.WTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/localCallerImg"
            android:layout_marginLeft="16dp"
            android:text="@string/txt_local_caller"
            android:textSize="15sp"
            android:textColor="@color/black"
            app:WTextView_font="myriadpro_regular"
            android:layout_centerVertical="true"
            />
        <za.co.woolworths.financial.services.android.ui.views.WTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:text="@string/customer_service_local_caller_number"
            android:textSize="15sp"
            android:textColor="#4d000000"
            app:WTextView_font="myriadpro_regular"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:drawableRight="@drawable/ic_caret_black"
            android:gravity="center_vertical"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#0a000000"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/internationalCaller"
        android:minHeight="50dp"
        android:paddingLeft="16dp"
        android:paddingRight="8dp"
        android:background="@color/white"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_call24"
            android:id="@+id/internationalCallerImg"
            android:layout_centerVertical="true"
            />
        <za.co.woolworths.financial.services.android.ui.views.WTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/internationalCallerImg"
            android:layout_marginLeft="16dp"
            android:text="@string/txt_international_caller"
            android:textSize="15sp"
            android:textColor="@color/black"
            app:WTextView_font="myriadpro_regular"
            android:layout_centerVertical="true"
            />
        <za.co.woolworths.financial.services.android.ui.views.WTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:text="@string/customer_service_inter_national_caller_number"
            android:textSize="15sp"
            android:textColor="#4d000000"
            app:WTextView_font="myriadpro_regular"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:drawableRight="@drawable/ic_caret_black"
            android:gravity="center_vertical"
            />
    </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#0a000000"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#0a000000"
            android:layout_marginTop="16dp"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="50dp"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:background="@color/white"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/ic_fax"
                android:id="@+id/faxImg"
                android:layout_centerVertical="true"
                />
            <za.co.woolworths.financial.services.android.ui.views.WTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/faxImg"
                android:layout_marginLeft="16dp"
                android:text="@string/fax"
                android:textSize="15sp"
                android:textColor="@color/black"
                app:WTextView_font="myriadpro_regular"
                android:layout_centerVertical="true"
                />
            <za.co.woolworths.financial.services.android.ui.views.WTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:text="@string/customer_service_inter_national_caller_number"
                android:textSize="15sp"
                android:textColor="#4d000000"
                app:WTextView_font="myriadpro_regular"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:gravity="center_vertical"
                />
        </RelativeLayout>

    <za.co.woolworths.financial.services.android.ui.views.WTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="68dp"
        android:paddingTop="20dp"
        android:background="@drawable/border_top_and_bottom"
        android:paddingLeft="16dp"
        android:text="@string/txt_call_center_opening_hours"
        android:textColor="@color/black"
        app:WTextView_font="medium_bold"
        android:textSize="12sp"
        android:letterSpacing="0.2"
        android:gravity="center_vertical"
        android:textAllCaps="true"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/white"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="10dp"
        android:paddingBottom="6dp"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_time24"
            android:layout_centerVertical="true"
            />
        <za.co.woolworths.financial.services.android.ui.views.WTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="122dp"
            android:text="@string/general_call_center_opening_hours"
            android:textColor="@color/black"
            app:WTextView_font="myriadpro_regular"
            android:textSize="15sp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="16dp"
            />
    </LinearLayout>
    <za.co.woolworths.financial.services.android.ui.views.WTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="68dp"
        android:paddingTop="20dp"
        android:background="@drawable/border_top_and_bottom"
        android:paddingLeft="16dp"
        android:text="@string/email_us"
        android:textColor="@color/black"
        app:WTextView_font="medium_bold"
        android:textSize="12sp"
        android:letterSpacing="0.2"
        android:gravity="center_vertical"
        android:textAllCaps="true"
        />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/productQuery"
            android:minHeight="70dp"
            android:padding="16dp"
            android:background="@color/white"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/email1"
                android:background="@drawable/message_24"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_toLeftOf="@+id/img1"
                android:layout_toRightOf="@+id/email1"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                >
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_header_textview"
                    android:text="@string/txt_product_query"
                    />
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_desc_textview"
                    android:layout_marginTop="5dp"
                    android:text="@string/email_custserv"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/img1"
                android:background="@drawable/ic_caret_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#0a000000"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/storeQuery"
            android:minHeight="70dp"
            android:padding="16dp"
            android:background="@color/white"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/email2"
                android:background="@drawable/message_24"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_toLeftOf="@+id/img2"
                android:layout_toRightOf="@+id/email2"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                >
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_header_textview"
                    android:text="@string/txt_store_query"
                    />
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_desc_textview"
                    android:layout_marginTop="5dp"
                    android:text="@string/email_custserv"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/img2"
                android:background="@drawable/ic_caret_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#0a000000"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/complaints"
            android:minHeight="70dp"
            android:padding="16dp"
            android:background="@color/white"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/email3"
                android:background="@drawable/message_24"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_toLeftOf="@+id/img3"
                android:layout_toRightOf="@+id/email3"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                >
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_header_textview"
                    android:text="@string/txt_complaint"
                    />
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_desc_textview"
                    android:layout_marginTop="5dp"
                    android:text="@string/email_custserv"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/img3"
                android:background="@drawable/ic_caret_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#0a000000"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/technicalProblem"
            android:minHeight="70dp"
            android:padding="16dp"
            android:background="@color/white"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/email4"
                android:background="@drawable/message_24"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_toLeftOf="@+id/img4"
                android:layout_toRightOf="@+id/email4"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                >
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_header_textview"
                    android:text="@string/txt_general_technical_problem"
                    />
                <za.co.woolworths.financial.services.android.ui.views.WTextView
                    style="@style/contact_us_item_desc_textview"
                    android:layout_marginTop="5dp"
                    android:text="@string/email_custserv"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/img4"
                android:background="@drawable/ic_caret_black"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />

        </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#0a000000"
        />

    </LinearLayout>

</ScrollView>