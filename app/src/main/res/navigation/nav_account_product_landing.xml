<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_account_product_landing"
    app:startDestination="@id/accountProductsMainFragment">

    <fragment
        android:id="@+id/accountProductsMainFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.main.AccountProductsMainFragment"
        android:label="AccountProductsMainFragment"
        tools:layout="@layout/account_product_landing_main_fragment">
        <action
            android:id="@+id/action_accountProductsMainFragment_to_accountProductsHomeFragment"
            app:destination="@id/accountProductsHomeFragment" />
        <action
            android:id="@+id/action_accountProductsMainFragment_to_accountInArrearsLandingDialogFragment"
            app:destination="@id/accountInArrearsLandingDialogFragment" />
        <action
            android:id="@+id/action_accountProductsMainFragment_to_removeBlockOnCollectionFragment2"
            app:destination="@id/removeBlockOnCollectionFragment2" />
    </fragment>
    <fragment
        android:id="@+id/accountProductsHomeFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.landing.AccountProductsHomeFragment"
        android:label="AccountProductsHomeFragment"
        tools:layout="@layout/account_products_home_fragment" />

    <dialog
        android:id="@+id/accountInArrearsLandingDialogFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.main.dialog.AccountInArrearsLandingDialogFragment"
        android:label="AccountInArrearsLandingDialogFragment"
        tools:layout="@layout/account_in_arrears_landing_dialog_fragment">
        <argument
            android:name="account"
            app:argType="za.co.woolworths.financial.services.android.models.dto.Account"
            app:nullable="true" />
    </dialog>
    <fragment
        android:id="@+id/removeBlockOnCollectionFragment2"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.remove_dc_block.RemoveBlockOnCollectionFragment"
        android:label="RemoveBlockOnCollectionFragment"
        tools:layout="@layout/remove_block_dc_main_fragment" />

</navigation>