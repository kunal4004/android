<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_account_product_landing"
    app:startDestination="@id/accountProductsMainFragment">

    <fragment
        android:id="@+id/accountProductsMainFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.main.AccountProductsMainFragment"
        android:label="AccountProductsMainFragment"
        tools:layout="@layout/account_product_landing_main_fragment">
        <action
            android:id="@+id/action_accountProductsMainFragment_to_accountProductsHomeFragment"
            app:destination="@id/accountProductsHomeFragment" />
        <action
            android:id="@+id/action_accountProductsMainFragment_to_accountLandingDialogFragment"
            app:destination="@id/accountLandingDialogFragment" />
        <action
            android:id="@+id/action_accountProductsMainFragment_to_removeBlockOnCollectionFragment"
            app:destination="@id/removeBlockOnCollectionFragment" />

        <action
            android:id="@+id/action_accountProductsMainFragment_to_accountInformationFragment"
            app:destination="@id/accountInfoFragment"
            app:enterAnim="@anim/slide_up_anim"
            app:popExitAnim="@anim/slide_down_anim"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/accountProductsHomeFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.landing.AccountProductsHomeFragment"
        android:label="AccountProductsHomeFragment"
        tools:layout="@layout/account_products_home_fragment">
        <action
            android:id="@+id/action_accountProductsHomeFragment_to_temporaryFreezeCardFragment"
            app:destination="@id/temporaryFreezeCardFragment" />
        <action
            android:id="@+id/action_accountProductsHomeFragment_to_nav_store_card_temporary_freeze_unfreeze_card"
            app:destination="@id/nav_store_card_temporary_freeze_unfreeze_card" />
        <action
            android:id="@+id/action_accountProductsHomeFragment_to_manageMyCardDetailsFragment"
            app:destination="@id/manageMyCardDetailsFragment" />
        <action
            android:id="@+id/action_accountProductsHomeFragment_to_accountLandingDialogFragment"
            app:destination="@id/accountLandingDialogFragment" />
        <action
            android:id="@+id/action_accountProductsHomeFragment_to_accountInfoFragment"
            app:destination="@id/accountInfoFragment" />
        <action
            android:id="@+id/action_accountProductsHomeFragment_to_toCardDetailFragmentDialog"
            app:destination="@id/toCardDetailFragmentDialog" />
        <action
            android:id="@+id/action_accountProductsHomeFragment_to_payMyAccountRetryErrorFragment"
            app:destination="@id/payMyAccountRetryErrorFragment" />
    </fragment>

    <dialog
        android:id="@+id/accountLandingDialogFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.main.dialog.AccountLandingDialogFragment"
        android:label="AccountLandingDialogFragment"
        tools:layout="@layout/account_landing_dialog_fragment">
        <argument
            android:name="account"
            app:argType="za.co.woolworths.financial.services.android.models.dto.Account"
            app:nullable="true" />
        <argument
            android:name="dialogData"
            app:argType="za.co.woolworths.financial.services.android.ui.fragments.account.main.domain.sealing.DialogData"
            app:nullable="true" />
        <argument
            android:name="eligibilityPlan"
            app:argType="za.co.woolworths.financial.services.android.models.dto.EligibilityPlan"
            app:nullable="true" />
    </dialog>
    <fragment
        android:id="@+id/removeBlockOnCollectionFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.remove_dc_block.AccountInDelinquencyFragment"
        android:label="RemoveBlockOnCollectionFragment"
        tools:layout="@layout/remove_block_dc_main_fragment" >
        <action
            android:id="@+id/action_removeBlockOnCollectionFragment_to_accountLandingDialogFragment"
            app:destination="@id/accountLandingDialogFragment" />
        <action
            android:id="@+id/action_removeBlockOnCollectionFragment_to_accountInfoFragment"
            app:destination="@id/accountInfoFragment" />
        <action
            android:id="@+id/action_removeBlockOnCollectionFragment_to_toCardDetailFragmentDialog"
            app:destination="@id/toCardDetailFragmentDialog" />
        <action
            android:id="@+id/action_removeBlockOnCollectionFragment_to_payMyAccountRetryErrorFragment"
            app:destination="@id/payMyAccountRetryErrorFragment" />
    </fragment>

    <fragment
        android:id="@+id/accountInfoFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.information.InformationFragment"
        android:label="AccountInformationFragment"
        tools:layout="@layout/fragment_information">
        <argument
            android:name="informationData"
            app:argType="za.co.woolworths.financial.services.android.ui.fragments.account.main.domain.sealing.InformationData"
            app:nullable="false" />
    </fragment>
    <include app:graph="@navigation/nav_store_card_temporary_freeze_unfreeze_card" />
    <fragment
        android:id="@+id/manageMyCardDetailsFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.account_options.feature_manage_card.card.ManageMyCardDetailsFragment"
        android:label="ManageMyCardDetailsFragment"
        tools:layout="@layout/manage_card_details_fragment" >
        <action
            android:id="@+id/action_manageMyCardDetailsFragment_to_accountProductsHomeFragment"
            app:destination="@id/accountProductsHomeFragment" />
    </fragment>

    <dialog
        android:id="@+id/toCardDetailFragmentDialog"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.dialog.ShowAmountPopupFragment"
        android:label="EnterPaymentAmountDetailFragment"
        tools:layout="@layout/pma_update_payment_fragment">
        <argument
            android:name="isAccountStartDestination"
            android:defaultValue="false"
            app:argType="boolean" />
    </dialog>
    <dialog
        android:id="@+id/payMyAccountRetryErrorFragment"
        android:name="za.co.woolworths.financial.services.android.ui.views.actionsheet.PayMyAccountRetryErrorFragment"
        android:label="PayMyAccountRetryErrorFragment"
        tools:layout="@layout/pay_my_account_retry_on_error_dialog_fragment" />
</navigation>