<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/checkout_nav_host"
    app:startDestination="@+id/CheckoutAddAddressNewUserFragment">

    <fragment
        android:id="@+id/CheckoutAddAddressNewUserFragment"
        android:name="za.co.woolworths.financial.services.android.checkout.view.CheckoutAddAddressNewUserFragment"
        android:label="Checkout New User"
        tools:layout="@layout/checkout_add_address_new_user">

        <action
            android:id="@+id/action_to_suburbSelectorFragment"
            app:destination="@id/suburbSelectorFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />

        <action
            android:id="@+id/action_to_provinceSelectorFragment"
            app:destination="@id/provinceSelectorFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />
        <action
            android:id="@+id/action_CheckoutAddAddressNewUserFragment_to_checkoutAddressConfirmationFragment"
            app:destination="@id/checkoutAddressConfirmationFragment" />

        <action
            android:id="@+id/action_to_unsellableItemsFragment"
            app:destination="@id/unsellableItemsFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />

        <action
            android:id="@+id/action_CheckoutAddAddressNewUserFragment_to_suburbNotDeliverableBottomsheetDialogFragment"
            app:destination="@id/suburbNotDeliverableBottomsheetDialogFragment"
            app:enterAnim="@anim/slide_in_up"
            app:exitAnim="@anim/stay"
            app:popEnterAnim="@anim/stay"
            app:popExitAnim="@anim/slide_down_anim" />

        <action
            android:id="@+id/action_CheckoutAddAddressNewUserFragment_to_ErrorHandlerBottomSheetDialog"
            app:destination="@id/errorHandlerBottomSheetDialog" />
        <action
            android:id="@+id/action_CheckoutAddAddressNewUserFragment_to_CheckoutAddAddressReturningUserFragment"
            app:destination="@id/CheckoutAddAddressReturningUserFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />

    </fragment>

    <fragment
        android:id="@+id/CheckoutAddAddressReturningUserFragment"
        android:name="za.co.woolworths.financial.services.android.checkout.view.CheckoutAddAddressReturningUserFragment"
        android:label="Checkout Returning User"
        tools:layout="@layout/checkout_add_address_retuning_user">
        <action
            android:id="@+id/action_CheckoutAddAddressReturningUserFragment_to_checkoutAddressConfirmationFragment"
            app:destination="@id/checkoutAddressConfirmationFragment" />

        <action
            android:id="@+id/action_CheckoutAddAddressReturningUserFragment_to_ErrorHandlerBottomSheetDialog"
            app:destination="@id/errorHandlerBottomSheetDialog" />
    </fragment>

    <fragment
        android:id="@+id/OrderConfirmationFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.product.shop.OrderConfirmationFragment"
        android:label="Checkout Order Confirmation"
        tools:layout="@layout/fragment_order_confirmation">

    </fragment>

    <fragment
        android:id="@+id/checkoutAddressConfirmationFragment"
        android:name="za.co.woolworths.financial.services.android.checkout.view.CheckoutAddressConfirmationFragment"
        android:label="Checkout Address Confirmation"
        tools:layout="@layout/checkout_address_confirmation">
        <action
            android:id="@+id/action_checkoutAddressConfirmationFragment_to_CheckoutAddAddressNewUserFragment"
            app:destination="@id/CheckoutAddAddressNewUserFragment" />

        <action
            android:id="@+id/action_getSuburb_suburbSelectorFragment"
            app:destination="@id/suburbSelectorFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right"
            app:popUpTo="@id/checkoutAddressConfirmationFragment" />

        <action
            android:id="@+id/action_provinceSelectorFragment"
            app:destination="@id/provinceSelectorFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />

        <action
            android:id="@+id/action_to_unsellableItemsFragment"
            app:destination="@id/unsellableItemsFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />
        <action
            android:id="@+id/action_checkoutAddressConfirmationFragment_to_CheckoutAddAddressReturningUserFragment"
            app:destination="@id/CheckoutAddAddressReturningUserFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right"/>
        <action
            android:id="@+id/action_checkoutAddressConfirmationFragment_to_suburbNotDeliverableBottomsheetDialogFragment"
            app:destination="@id/suburbNotDeliverableBottomsheetDialogFragment" />
    </fragment>

    <fragment
        android:id="@+id/unsellableItemsFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.click_and_collect.UnsellableItemsFragment"
        android:label="UnsellableItemsFragment"
        tools:layout="@layout/unsellable_items_fragment" />

    <fragment
        android:id="@+id/suburbSelectorFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.click_and_collect.SuburbSelectorFragment"
        android:label="SuburbSelectorFragment"
        tools:layout="@layout/suburb_selector_fragment" />

    <fragment
        android:id="@+id/provinceSelectorFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.click_and_collect.ProvinceSelectorFragment"
        android:label="ProvinceSelectorFragment"
        tools:layout="@layout/province_selector_fragment">

        <action
            android:id="@+id/action_suburbSelectorFragment"
            app:destination="@id/suburbSelectorFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right"
            app:popUpTo="@id/checkoutAddressConfirmationFragment" />

    </fragment>
    <dialog
        android:id="@+id/suburbNotDeliverableBottomsheetDialogFragment"
        android:name="za.co.woolworths.financial.services.android.checkout.view.SuburbNotDeliverableBottomsheetDialogFragment"
        android:label="SuburbNotDeliverableBottomsheetDialogFragment"
        tools:layout="@layout/fragment_suburb_not_deliverable_bottomsheet_dialog"/>
    <dialog
        android:id="@+id/errorHandlerBottomSheetDialog"
        android:name="za.co.woolworths.financial.services.android.checkout.view.ErrorHandlerBottomSheetDialog"
        android:label="ErrorHandlerBottomSheetDialog" />

</navigation>