<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_account_options_list"
    app:startDestination="@id/accountOptionsListFragment">

    <fragment
        android:id="@+id/accountOptionsListFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.account_options.feature_account_options_list.AccountOptionsListFragment"
        android:label="AccountOptionsListFragment"
        tools:layout="@layout/account_options_list_fragment" >
        <action
            android:id="@+id/action_accountOptionsListFragment_to_payMyAccountRetryErrorFragment"
            app:destination="@id/payMyAccountRetryErrorFragment" />
        <action
            android:id="@+id/action_accountOptionsListFragment_to_toCardDetailFragmentDialog"
            app:destination="@id/toCardDetailFragmentDialog" />
    </fragment>

    <dialog
        android:id="@+id/payMyAccountRetryErrorFragment"
        android:name="za.co.woolworths.financial.services.android.ui.views.actionsheet.PayMyAccountRetryErrorFragment"
        android:label="PayMyAccountRetryErrorFragment"
        tools:layout="@layout/pay_my_account_retry_on_error_dialog_fragment" />

    <dialog
        android:id="@+id/toCardDetailFragmentDialog"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.dialog.ShowAmountPopupFragment"
        android:label="EnterPaymentAmountDetailFragment"
        tools:layout="@layout/pma_update_payment_fragment"/>

</navigation>