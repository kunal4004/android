<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pay_my_account_nav_host"
    app:startDestination="@id/creditAndDebitCardPaymentsFragment">

    <fragment
        android:id="@+id/creditAndDebitCardPaymentsFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.CreditAndDebitCardPaymentsFragment"
        android:label="CreditAndDebitCardPaymentsFragment"
        tools:layout="@layout/credit_and_debit_card_payments_fragment">
        <action
            android:id="@+id/action_creditAndDebitCardPaymentsFragment_to_payMyAccountLearnMoreFragment"
            app:destination="@id/payMyAccountLearnMoreFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />

        <action
            android:id="@+id/action_creditAndDebitCardPaymentsFragment_to_byElectronicFundTransferFragment"
            app:destination="@id/byElectronicFundTransferFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />
        <action
            android:id="@+id/action_creditAndDebitCardPaymentsFragment_to_storesNearbyFragment1"
            app:destination="@id/storesNearbyFragment1"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />
        <action
            android:id="@+id/goToEnterPaymentAmountFragmentAction"
            app:destination="@id/enterPaymentAmountFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right"></action>
    </fragment>
    <fragment
        android:id="@+id/payMyAccountLearnMoreFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.PayMyAccountLearnMoreFragment"
        android:label="PayMyAccountLearnMoreFragment"
        tools:layout="@layout/pay_my_account_learn_more_fragment" />
    <fragment
        android:id="@+id/byElectronicFundTransferFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.ByElectronicFundTransferFragment"
        android:label="ByElectronicFundTransferFragment"
        tools:layout="@layout/pma_by_electronic_fund_transfer_eft_fragment" />
    <fragment
        android:id="@+id/storesNearbyFragment1"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.store.StoresNearbyFragment1"
        android:label="StoresNearbyFragment1"
        tools:layout="@layout/fragment_stores_nearby1" />
    <fragment
        android:id="@+id/enterPaymentAmountFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.EnterPaymentAmountFragment"
        android:label="EnterPaymentAmountFragment"
        tools:layout="@layout/enter_payment_amount_fragment">
        <argument
            android:name="ACCOUNT"
            app:argType="za.co.woolworths.financial.services.android.models.dto.Account" />
        <action
            android:id="@+id/action_enterPaymentAmountFragment_to_addNewPayUCardFragment"
            app:destination="@id/addNewPayUCardFragment" />
        <action
            android:id="@+id/processYourRequestFragmentAction"
            app:destination="@id/processYourRequestFragment"
            app:launchSingleTop="true" />
        <action
            android:id="@+id/action_enterPaymentAmountFragment_to_saveCardAndPayNowFragment"
            app:destination="@id/saveCardAndPayNowFragment" />
    </fragment>
    <fragment
        android:id="@+id/addNewPayUCardFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.AddNewPayUCardFragment"
        android:label="AddNewPayUCardFragment"
        tools:layout="@layout/add_new_payu_card_fragment">
        <argument
            android:name="ACCOUNT"
            app:argType="za.co.woolworths.financial.services.android.models.dto.Account" />
        <action
            android:id="@+id/addNewPayUCardFragment_to_processYourRequestFragmentAction"
            app:destination="@id/processYourRequestFragment" />
    </fragment>
    <fragment
        android:id="@+id/processYourRequestFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.ProcessYourRequestFragment"
        android:label="ProcessYourRequestFragment"
        tools:layout="@layout/process_your_request_fragment">
        <argument
            android:name="ACCOUNT"
            app:argType="za.co.woolworths.financial.services.android.models.dto.Account" />
        <action
            android:id="@+id/processPaymentSuccessFragmentAction"
            app:destination="@id/processPaymentSuccessFragment" />
        <action
            android:id="@+id/processPaymentFailureFragmentAction"
            app:destination="@id/processPaymentFailureFragment" />
        <action
            android:id="@+id/action_processYourRequestFragment_to_saveCardAndPayNowFragment"
            app:destination="@id/saveCardAndPayNowFragment" />
    </fragment>
    <fragment
        android:id="@+id/processPaymentSuccessFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.ProcessPaymentSuccessFragment"
        android:label="ProcessPaymentSuccessFragment"
        tools:layout="@layout/process_payment_success_fragment">
        <action
            android:id="@+id/action_processPaymentSuccessFragment_to_processYourRequestFragment"
            app:destination="@id/processYourRequestFragment" />
    </fragment>
    <fragment
        android:id="@+id/processPaymentFailureFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.ProcessPaymentFailureFragment"
        android:label="ProcessPaymentFailureFragment"
        tools:layout="@layout/process_payment_failure_fragment">
        <action
            android:id="@+id/action_processPaymentFailureFragment_to_processYourRequestFragment"
            app:destination="@id/processYourRequestFragment" />
    </fragment>
    <fragment
        android:id="@+id/saveCardAndPayNowFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.detail.pay_my_account.SaveCardAndPayNowFragment"
        android:label="SaveCardAndPayNowFragment"
        tools:layout="@layout/save_card_and_pay_now_fragment" >
        <action
            android:id="@+id/action_saveCardAndPayNowFragment_to_processPaymentSuccessFragment"
            app:destination="@id/processPaymentSuccessFragment" />
    </fragment>

</navigation>