<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/account_options_manage_card_fragment"
    app:startDestination="@id/accountOptionsManageCardFragment">

    <fragment
        android:id="@+id/accountOptionsManageCardFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.account_options.AccountOptionsManageCardFragment"
        android:label="AccountOptionsManageCardFragment"
        tools:layout="@layout/account_options_manage_card_fragment">
        <action
            android:id="@+id/action_accountOptionsManageCardFragment_to_temporaryFreezeCardFragment"
            app:destination="@id/temporaryFreezeCardFragment" />
        <action
            android:id="@+id/action_accountOptionsManageCardFragment_to_temporaryUnFreezeCardFragment"
            app:destination="@id/temporaryUnFreezeCardFragment" />
        <action
            android:id="@+id/action_accountOptionsManageCardFragment_to_manageCardViewPagerFragment"
            app:destination="@id/manageCardViewPagerFragment" />
        <action
            android:id="@+id/action_accountOptionsManageCardFragment_to_generalErrorDialogPopupFragment"
            app:destination="@id/generalErrorDialogPopupFragment" />
    </fragment>
    <fragment
        android:id="@+id/manageCardViewPagerFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.account_options.feature_manage_card.card.ManageCardViewPagerFragment"
        android:label="ManageCardViewPagerFragment"
        tools:layout="@layout/manage_card_viewpager_fragment" />
    <dialog
        android:id="@+id/generalErrorDialogPopupFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.account_options.error_handler.GeneralErrorDialogPopupFragment"
        android:label="GeneralErrorDialogPopupFragment"
        tools:layout="@layout/general_error_dialog_popup_fragment">
        <argument
            android:name="serverErrorResponse"
            app:argType="za.co.woolworths.financial.services.android.models.dto.account.ServerErrorResponse" />
    </dialog>
    <fragment
        android:id="@+id/payWithCardListFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.account_options.feature_manage_card.card.PayWithCardListFragment"
        android:label="PayWithCardListFragment"
        tools:layout="@layout/pay_with_card_list_fragment" >
        <action
            android:id="@+id/action_payWithCardListFragment_to_scanBarcodeToPayDialogFragment"
            app:destination="@id/scanBarcodeToPayDialogFragment" />
    </fragment>
    <dialog
        android:id="@+id/scanBarcodeToPayDialogFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.account.main.ui.fragment.account_options.feature_manage_card.detail.ScanBarcodeToPayDialogFragment"
        android:label="ScanBarcodeToPayDialogFragment"
        tools:layout="@layout/scan_barcode_to_pay_dialog" >
        <argument
            android:name="storeCardResponse"
            app:argType="za.co.woolworths.financial.services.android.models.dto.temporary_store_card.StoreCardsResponse" />
    </dialog>

</navigation>