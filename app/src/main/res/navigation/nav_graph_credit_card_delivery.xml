<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_credit_card_delivery"
    app:startDestination="@id/creditCardDeliveryBoardingFragment">

    <fragment
        android:id="@+id/creditCardDeliveryBoardingFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.credit_card_delivery.CreditCardDeliveryBoardingFragment"
        android:label="CreditCardDeliveryBoardingFragment"
        tools:layout="@layout/credit_card_delivery_boarding_layout">
        <action
            android:id="@+id/action_to_creditCardDeliveryRecipientDetailsFragment"
            app:destination="@id/creditCardDeliveryRecipientDetailsFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right"
            app:popUpTo="@+id/creditCardDeliveryBoardingFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/creditCardDeliveryManageDeliveryFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.credit_card_delivery.CreditCardDeliveryManageDeliveryFragment"
        android:label="CreditCardDeliveryManageDeliveryFragment"
        tools:layout="@layout/credit_card_delivery_manage_delivery" />
    <fragment
        android:id="@+id/creditCardDeliveryStatusFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.credit_card_delivery.CreditCardDeliveryStatusFragment"
        android:label="CreditCardDeliveryStatusFragment"
        tools:layout="@layout/credit_card_delivery_status_layout">
        <action
            android:id="@+id/action_to_creditCardDeliveryManageDeliveryFragment"
            app:destination="@id/creditCardDeliveryManageDeliveryFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right" />
    </fragment>
    <fragment
        android:id="@+id/creditCardDeliveryAddressDetailsFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.credit_card_delivery.CreditCardDeliveryAddressDetailsFragment"
        android:label="CreditCardDeliveryAddressDetailsFragment"
        tools:layout="@layout/credit_card_delivery_recipient_address_layout">
        <action
            android:id="@+id/action_to_creditCardDeliveryValidateAddressRequestFragment"
            app:destination="@id/creditCardDeliveryValidateAddressRequestFragment"
            app:popUpTo="@+id/creditCardDeliveryRecipientDetailsFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/creditCardDeliveryRecipientDetailsFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.credit_card_delivery.CreditCardDeliveryRecipientDetailsFragment"
        android:label="CreditCardDeliveryRecipientDetailsFragment"
        tools:layout="@layout/credit_card_delivery_recipient_details_layout">
        <action
            android:id="@+id/action_to_creditCardDeliveryAddressDetailsFragment"
            app:destination="@id/creditCardDeliveryAddressDetailsFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right"
            app:popUpTo="@+id/creditCardDeliveryRecipientDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/creditCardDeliveryValidateAddressRequestFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.credit_card_delivery.CreditCardDeliveryValidateAddressRequestFragment"
        android:label="CreditCardDeliveryValidateAddressRequestFragment">

        <action
            android:id="@+id/action_to_creditCardDeliveryPreferedTimeslotFragment"
            app:destination="@id/creditCardDeliveryPreferedTimeslotFragment"
            app:popUpTo="@+id/creditCardDeliveryRecipientDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/creditCardDeliveryPreferedTimeslotFragment"
        android:name="za.co.woolworths.financial.services.android.ui.fragments.credit_card_delivery.CreditCardDeliveryPreferedTimeslotFragment"
        android:label="CreditCardDeliveryPreferedTimeslotFragment">
        <action
            android:id="@+id/action_creditCardDeliveryPreferedTimeslotFragment_to_creditCardDeliveryStatusFragment"
            app:destination="@id/creditCardDeliveryStatusFragment" />
    </fragment>
</navigation>